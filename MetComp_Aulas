      "source": [
        "# area da coroa\n",
        "\n",
        "from math import *\n",
        "\n",
        "def areac(r1):\n",
        "  #calcula area do circulo\n",
        "  return pi*r1**2\n",
        "def coroa(r1,r2):\n",
        "  \"\"\" calcula area de coroa formada \n",
        "  pelos circulos de raio r1 e r2\n",
        "  com r1>r2 \"\"\"\n",
        "  if r1>r2:\n",
        "    print (\"r1 deve ser menor que r2\")\n",
        "    return 0 \n",
        "  else: \n",
        "    return areac(r2) - areac(r1)\n",
        "\n",
        "print (\"a area e %5.2f \\n\" %(coroa(r1=2,r2=3)))"
      ],
      "execution_count": null,
