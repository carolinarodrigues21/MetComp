      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "def f1(t):\n",
        "  return t**2*np.exp(-t**2)\n",
        "\n",
        "def f2(t):\n",
        "  return t**2*f1(t)\n",
        "\n",
        "t = np.linspace(0,3,51) #vai de zero a 3 com 51 pontos\n",
        "y1 = f1(t)\n",
        "y2 = f2(t)\n",
        "\n",
        "plt.plot(t, y1, 'r-')\n",
        "plt.plot(t,y2,'bo')\n",
        "plt.xlabel('t [s]')\n",
        "plt.ylabel('y [m]')\n",
        "plt.legend(['tˆ2*exp(-tˆ2)',\\\n",
        "            'tˆ4*exp(-tˆ2)'])\n",
        "plt.title('Duas curvas')  \n",
        "plt.show()"
      ],
